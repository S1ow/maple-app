.grace-left-padding-wrap .grace-list view text{color:#666; font-size:28rpx;}
@-webkit-keyframes gradient{50%{background-position:100% 0;}}
@keyframes gradient{50%{background-position:100% 0;}}
.grace-gradient-bg{width:100%;
  background: -o-linear-gradient(45deg,#5a3694 0,#13bdce 33%,#0094d9 66%,#6fc7b5 100%);
